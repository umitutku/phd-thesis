@article{CHOI2001177,
    title = "Highly sensitive radon monitor and radon emanation rates for detector components",
    journal = "Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment",
    volume = "459",
    number = "1",
    pages = "177 - 181",
    year = "2001",
    issn = "0168-9002",
    doi = "https://doi.org/10.1016/S0168-9002(00)01004-4",
    url = "http://www.sciencedirect.com/science/article/pii/S0168900200010044",
    author = "E. Choi and M. Komori and K. Takahisa and N. Kudomi and K. Kume and K. Hayashi and S. Yoshida and H. Ohsumi and H. Ejiri and T. Kishimoto and K. Matsuoka and S. Tasaka",
    keywords = "High sensitive radon monitor, Radon concentration, Detector component",
    abstract = "The radon emanation rates for materials were measured by using the electrostatic precipitation method as a radon monitor. It was found that a low level of radon was emanated from several material components in ELEGANT V. The radon monitor has been developed for the highly sensitive measurements of low-level radon concentration. The system was shown to have a sensitivity to radon concentrations as low as 1.6mBq/m3 for one day measurement. The system was also used as the radon concentration monitor for the gas inside the airtight box of the ELEGANT V at Oto Cosmo Observatory."
}

%% Development of electrostatic detector used at UCL
@article{MITSUDA2003414,
    title = "Development of super-high sensitivity radon detector for the Super-Kamiokande detector",
    journal = "Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment",
    volume = "497",
    number = "2",
    pages = "414 - 428",
    year = "2003",
    issn = "0168-9002",
    doi = "https://doi.org/10.1016/S0168-9002(02)01923-X",
    url = "http://www.sciencedirect.com/science/article/pii/S016890020201923X",
    author = "C Mitsuda and T Kajita and K Miyano and S Moriyama and M Nakahata and Y Takeuchi and S Tasaka",
    keywords = "Radon, Super-Kamiokande, Electrostatic collection, Photodiode, Membrane",
    abstract = "A super-high sensitivity radon detector for water which works as a real-time monitor of the radon concentration in water for the Super-Kamiokande detector was developed. Super-high sensitivity was achieved through the utilization of the membrane degassifier module, in addition to factors such as large detector volume, high electrostatic collection voltage, and high-quality electronics. As a result of the calibration study, a sensitivity of 14.6±2.1(stat.+syst.) (counts/day)/(mBq/m3) is achieved. The background level of the radon detector for water is 34.5±5.8(stat.) (counts/day). This corresponds to the detection limit of 0.7mBq/m3 in a one-day measurement."
}

